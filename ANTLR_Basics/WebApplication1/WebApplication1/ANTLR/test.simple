// =======================================================
// TEST: String Funktion LeftRange
// =======================================================

// Vorbereitung (String: A B C D E F G)
// Indizes:     0 1 2 3 4 5 6
rangeString = "ABCDEFG"; 

// 29. Test: Starte bei Index 2 ('C'), Länge 3
var range1.Left(rangeString, 2, 3);
print("Test 29 (Start 2, Länge 3): ->");
print(range1); // Erwartet: CDE

// 30. Test: Starte bei Index 4 ('E'), Länge 5 (Überlappung)
var range2.Left(rangeString, 4, 5);
print("Test 30 (Start 4, Länge 5): ->");
print(range2); // Erwartet: EFGs
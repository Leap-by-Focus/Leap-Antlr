// Test 20: Min-Funktion
print("=== MIN-FUNKTION ===");

// Direkte Zahlenliste
var result1.Min(5, 3, 8, 1, 9);
print("Min(5, 3, 8, 1, 9) = " + result1);

// Mit Dezimalzahlen
var result2.Min(2.5, 1.8, 3.2, 0.5);
print("Min(2.5, 1.8, 3.2, 0.5) = " + result2);

// Nur eine Zahl
var result3.Min(7);
print("Min(7) = " + result3);

// Mit negativen Zahlen
var result4.Min(-5, -2, 0, 3);
print("Min(-5, -2, 0, 3) = " + result4);

// Gemischte Zahlen
var result5.Min(10, -3, 5.5, 0, -10.2);
print("Min(10, -3, 5.5, 0, -10.2) = " + result5);